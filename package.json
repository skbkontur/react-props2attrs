{
  "name": "@skbkontur/react-props2attrs",
  "version": "0.1.2",
  "description": "Translate react-props to attributes associated html-element",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc",
    "test": "npm run clean && jest",
    "clean": "git clean -fdxqe node_modules",
    "release": "npm run build && npm publish --access public",
    "lint": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skbkontur/react-props2attrs.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skbkontur/react-props2attrs/issues"
  },
  "homepage": "https://github.com/skbkontur/react-props2attrs#readme",
  "devDependencies": {
    "@testing-library/react": "^10.4.9",
    "@types/jest": "^25.2.3",
    "@types/react-dom": "^16.9.8",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-jest": "^26.4.0",
    "typescript": "^3.9.7"
  },
  "jest": {
    "transform": {
      "\\.tsx?$": [
        "ts-jest"
      ]
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.json"
      }
    },
    "testRegex": "test\\.tsx$"
  },
  "dependencies": {
    "@skbkontur/react-sorge": "0.1.0"
  }
}
